<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/container"
        android:layout_gravity="center"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        card_view:cardCornerRadius="4dp"
        card_view:cardElevation="5dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="4dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:weightSum="1.0"
            android:baselineAligned="false">

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="0.2"
                android:layout_marginTop="12dp"
                android:layout_marginStart="10dp"
                android:layout_marginBottom="12dp"
                android:layout_marginEnd="0dp"
                android:background="@color/colorLightPrimary"
                android:orientation="vertical"
                android:gravity="center">

                <TextView
                        android:id="@+id/bagQuantity"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="2dp"
                        android:gravity="center"
                        android:text="19:30"
                        android:textColor="@color/colorAccent"
                        android:textSize="@dimen/default_font_size"
                        android:fontFamily="@font/latobold"
                        android:transitionName="qty_tran"/>
                <TextView
                        android:layout_width="match_parent"
                        android:layout_height="15dp"
                        android:gravity="center"
                        android:text="Bag Qty"
                        android:textSize="11dp"
                        android:textColor="@color/colorTextTheme"
                        android:layout_marginTop="5dp"
                        android:fontFamily="@font/latoregular"
                        />

            </LinearLayout>

            <LinearLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="0.8"
                android:layout_marginStart="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:orientation="vertical">


                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:id="@+id/doNumber"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:text="DO Number"
                            android:textColor="@color/colorTextTheme"
                            android:textSize="@dimen/default_font_size"
                            android:fontFamily="@font/latobold"
                            android:layout_weight="0.4"
                            android:transitionName="do_no_tran"/>
                        <TextView
                            android:id="@+id/approve_date"
                            android:layout_width="0dp"
                            android:layout_weight="0.6"
                            android:layout_height="wrap_content"
                            android:text="Date"
                            android:textColor="@color/colorTextTheme"
                            android:textSize="@dimen/font_size_medium"
                            android:fontFamily="@font/latoregular"
                            android:layout_marginTop="0dp"
                            android:paddingEnd="10dp"
                            android:gravity="end"
                            android:transitionName="date_tran"/>


                    </LinearLayout>

                    <TextView
                        android:id="@+id/product_name"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="01710000000"
                        android:textSize="@dimen/font_size_medium"
                        android:textColor="@color/colorLightBlack"
                        android:fontFamily="@font/latoregular"
                        android:layout_marginTop="5dp"
                        android:transitionName="product_name_tran"/>
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <TextView
                            android:id="@+id/dealer_name_TV"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:text="Dealer Name"
                            android:textColor="@color/colorGray"
                            android:textSize="@dimen/font_size_medium"
                            android:fontFamily="@font/latoregular"
                            android:layout_marginTop="5dp"
                            android:transitionName="dealer_tran"/>
                        <TextView
                            android:id="@+id/status"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.5"
                            android:text="View Details"
                            android:textColor="#008080"
                            android:textSize="12sp"
                            android:fontFamily="@font/latobold"
                            android:layout_marginTop="5dp"
                            android:gravity="end"
                            android:paddingStart="2dp"
                            android:paddingEnd="10dp" />


                    </LinearLayout>



            </LinearLayout>

